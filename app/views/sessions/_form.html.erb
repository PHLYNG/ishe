<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :email, class: 'new-streets new-streets-label' %>
  <%= f.email_field :email, class: 'form-control new-streets new-streets-field'  %>

  <%= f.label :password, class: 'new-streets new-streets-label' %>
  <%= f.password_field :password, class: 'form-control new-streets new-streets-field'  %>

  <button class="btn btn-success"><%= f.submit "Login to My Account" %></button>
<% end %>
